/Codechef SnackDown 2021 Online Qualifiers: Lucky Number/

/Below is the solution for the above problem/

#include<iostream>
using namespace std;
void solve()
{
    int a,b,c;
    cin>>a>>b>>c;
    if(a==7 || b==7 || c==7){
        cout<<"YES\n";
    }
    else{
        cout<<"NO\n";
    }
}
int main()
{
    int T;
    cin>>T;
    while(T--)
    {
        solve();
    }
    return 0;
}
